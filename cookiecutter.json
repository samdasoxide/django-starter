{
    "project_name": "project_name",
    "project_slug": "{{ cookiecutter.project_name.lower()|replace(' ', '_')|replace('-', '_') }}",
    "repo_url": "git://github.com/rocketpod/django-starter.git",
    "description": "A short description of the project.",
    "wagtail": ["y","n"],
    "opbeat": ["y","n"],
    "sentry": ["y","n"],
    "domain_name": "example.com",
    "_copy_without_render": [
        "*.html",
        "*.pyc",
        "*gulpfile.js/lib",
        "*gulpfile.js/tasks",
        "*node_modules",
        "*provisioning/development.yml",
        "*provisioning/group_vars/all/vars.yml",
        "*provisioning/group_vars/all/vault.yml",
        "*provisioning/group_vars/development.yml",
        "*provisioning/group_vars/production.yml",
        "*provisioning/group_vars/staging.yml",
        "*provisioning/production.yml",
        "*provisioning/production.yml",
        "*provisioning/roles",
        "*provisioning/staging.yml",
        "*provisioning/vars",
        "*static_src"
    ]
}
